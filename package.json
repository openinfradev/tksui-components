{
    "name": "tksui-components",
    "version": "0.4.4",
    "private": false,
    "type": "module",
    "module": "lib/esm/index.js",
    "main": "lib/esm/index.js",
    "files": [
        "lib/"
    ],
    "scripts": {
        "clean": "rimraf lib",
        "build-tsc": "tsc --project tsconfig.esm.json && tsc-alias -p tsconfig.esm.json",
        "build-sass": "sass --no-source-map src/styles/Entry.scss lib/styles/index.css",
        "minify-js": "esbuild lib/esm/**/*.js --minify --allow-overwrite --outdir=lib/esm",
        "minify-css": "esbuild lib/styles/index.css --minify --allow-overwrite --outfile=lib/styles/index.css",
        "build-all": "npm run clean && npm run build-tsc && npm run build-sass && npm run minify-js && npm run minify-css",
        "patch": "npm run build-all && npm version patch && npm publish && npm run clean",
        "minor": "npm run build-all && npm version minor && npm publish && npm run clean",
        "major": "npm run build-all && npm version major && npm publish && npm run clean",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "test": "jest --watch",
        "test-all": "jest --watchAll"
    },
    "devDependencies": {
        "@babel/core": "^7.22.10",
        "@babel/preset-env": "^7.22.10",
        "@babel/preset-react": "^7.22.5",
        "@babel/preset-typescript": "^7.22.5",
        "@storybook/addon-actions": "^7.5.1",
        "@storybook/addon-essentials": "^7.5.1",
        "@storybook/addon-interactions": "^7.5.1",
        "@storybook/addon-links": "^7.5.1",
        "@storybook/preset-scss": "^1.0.3",
        "@storybook/react": "^7.5.1",
        "@storybook/react-vite": "^7.5.1",
        "@storybook/testing-library": "^0.2.2",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/jest": "^29.5.4",
        "@types/react": "^18.0.37",
        "@types/react-dom": "^18.0.11",
        "@types/react-slick": "^0.23.11",
        "@typescript-eslint/eslint-plugin": "^5.59.0",
        "@typescript-eslint/parser": "^5.59.0",
        "@vitejs/plugin-react": "^4.0.0",
        "esbuild": "^0.19.5",
        "eslint": "^8.38.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest-dom": "^4.0.3",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-react": "^7.31.11",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.3.4",
        "eslint-plugin-storybook": "^0.6.15",
        "eslint-plugin-testing-library": "^5.10.2",
        "fs-extra": "^11.1.1",
        "jest": "^29.6.3",
        "jest-environment-jsdom": "^29.6.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rimraf": "^5.0.1",
        "rollup-plugin-visualizer": "^5.9.2",
        "sass": "^1.69.1",
        "storybook": "^7.5.1",
        "ts-jest": "^29.1.1",
        "tsc-alias": "^1.8.7",
        "typescript": "^5.0.2",
        "vite": "^4.3.9",
        "vite-plugin-compression2": "^0.10.4"
    },
    "peerDependencies": {
        "react": "^18.0.0"
    },
    "dependencies": {
        "ag-grid-community": "^30.0.6",
        "ag-grid-react": "^30.0.6",
        "lodash": "^4.17.21",
        "material-icons": "^1.13.10",
        "react-modal": "^3.16.1",
        "react-slick": "^0.29.0",
        "react-toastify": "^9.1.3",
        "react-tooltip": "^5.21.1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/openinfradev/tksui-components.git"
    },
    "bugs": {
        "url": "https://github.com/openinfradev/tksui-components/issues"
    },
    "license": "MIT"
}
